<%= form_for @sheet do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :student_id %><br />
    <%= f.text_field :student_id %>
  </p>
  <p>
    <%= f.label :partner_id %><br />
    <%= f.text_field :partner_id %>
  </p>
  <p>
    <%= f.label :experiment_id %><br />
    <%= f.text_field :experiment_id %>
  </p>
  <p>
    <%= f.label :marker_id %><br />
    <%= f.text_field :marker_id %>
  </p>
  <p>
    <%= f.label :comments %><br />
    <%= f.text_field :comments %>
  </p>
  <p>
    <%= f.label :raw_mark %><br />
    <%= f.text_field :raw_mark %>
  </p>
  <p>
    <%= f.label :ret_mark %><br />
    <%= f.text_field :ret_mark %>
  </p>
  <p><%= f.submit %></p>
<% end %>

<table>
  <tr>
    <th>Student</th>
    <th>Partner</th>
    <th>Experiment</th>
    <th>Marker</th>
    <th>Comments</th>
    <th>Raw Mark</th>
    <th>Ret Mark</th>
  </tr>
  <% for sheet in @sheets %>
    <tr>
      <td><%= sheet.student_id %></td>
      <td><%= sheet.partner_id %></td>
      <td><%= sheet.experiment_id %></td>
      <td><%= sheet.marker_id %></td>
      <td><%= sheet.comments %></td>
      <td><%= sheet.raw_mark %></td>
      <td><%= sheet.ret_mark %></td>
      <td><%= link_to "Show", sheet %></td>
      <td><%= link_to "Edit", edit_sheet_path(sheet) %></td>
      <td><%= link_to "Destroy", sheet, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Sheet", new_sheet_path %></p>
